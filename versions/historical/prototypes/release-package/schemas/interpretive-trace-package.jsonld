  "properties": {
    "artifact_type": {
      "type": "string",
      "const": "InterpretiveTracePackage",
      "$comment": "Restricts the artifact to Interpretive Trace Package for AEIP governance traceability."
    },
    "version": {
      "type": "string",
      "pattern": "^\\d+\\.\\d+\\.\\d+$",
      "$comment": "Semantic version for the artifact payload to track interpretive evidence lineage."
    },
    "uuid": {
      "type": "string",
      "pattern": "^(urn:uuid:)?[0-9a-fA-F]{8}-[0-9a-fA-F]{4}-4[0-9a-fA-F]{3}-[89abAB][0-9a-fA-F]{3}-[0-9a-fA-F]{12}$",
      "$comment": "Mandatory UUIDv4 identifier ensuring collision-resistant trace references."
    },
    "timestamp": {
      "type": "string",
      "format": "date-time",
      "$comment": "Recorded in UTC to preserve temporal integrity for interpretive reviews."
    },
    "layer": {
      "type": "integer",
      "minimum": 1,
      "maximum": 7,
      "$comment": "Specifies the AI OSI Stack layer primarily addressed by the interpretive evidence."
    },
    "issuer": {
      "type": "object",
      "required": ["name", "role", "organization", "contact"],
      "properties": {
        "name": { "type": "string" },
        "role": { "type": "string" },
        "organization": { "type": "string" },
        "contact": { "type": "string", "format": "uri" }
      },
      "additionalProperties": false,
      "$comment": "Identifies the accountable interpretive authority submitting the trace."
    },
    "summary": {
      "type": "string",
      "$comment": "Narrative synopsis of the interpretive reasoning sequence."
    },
    "linked_artifacts": {
      "type": "array",
      "minItems": 1,
      "items": {
        "anyOf": [
          {
            "type": "string",
            "pattern": "^(urn:uuid:)?[0-9a-fA-F]{8}-[0-9a-fA-F]{4}-4[0-9a-fA-F]{3}-[89abAB][0-9a-fA-F]{3}-[0-9a-fA-F]{12}$"
          },
          {
            "type": "string",
            "format": "uri"
          }
        ]
      },
      "$comment": "References supporting materials, evidence nodes, or governance directives."
    },
    "signature": {
      "type": "object",
      "required": ["method", "key_id", "value"],
      "properties": {
        "method": { "type": "string" },
        "key_id": { "type": "string" },
        "value": { "type": "string" }
      },
      "additionalProperties": false,
      "$comment": "Cryptographic assertion binding the interpretive content to an accountable signer."
    },
    "provenance": {
      "type": "object",
      "required": ["source_system", "collection_method", "jurisdiction"],
      "properties": {
        "source_system": { "type": "string" },
        "collection_method": { "type": "string" },
        "jurisdiction": { "type": "string" },
        "confidence": { "type": "string" }
      },
      "additionalProperties": false,
      "$comment": "Documents the evidentiary path to support audit reconstruction and epistemic trust."
    }
  },
  "additionalProperties": false,
