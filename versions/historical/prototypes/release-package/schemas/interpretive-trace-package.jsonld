  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Interpretive Trace Package",
  "description": "JSON-LD schema for AEIP Interpretive Trace Package artifacts, enabling reconstruction of interpretive reasoning chains across the AI OSI Stack.",
  "type": "object",
  "required": [
    "artifact_type",
    "version",
    "uuid",
    "timestamp",
    "layer",
    "issuer",
    "summary",
    "linked_artifacts",
    "signature",
    "provenance"
