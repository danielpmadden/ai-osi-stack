{
  "name": "ai-osi-stack",
  "version": "5.0.0",
  "description": "Operational security, governance, and provenance tooling for the AI OSI Stack.",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=18.18.0"
  },
  "scripts": {
    "lint": "eslint .",
    "format": "prettier --check .",
    "test": "vitest run",
    "test:watch": "vitest",
    "validate:aeip": "tsx ops/aeip/validate-aeip.ts examples/aeip",
    "validate:governance": "tsx ops/aeip/validate-governance.ts examples/aeip",
    "convert:dashboard": "tsx ops/aeip/convert-for-dashboard.ts examples/aeip --out dist/dashboard-receipts.json"
  },
  "dependencies": {
    "ajv": "8.17.1",
    "ajv-formats": "3.0.1"
  },
  "devDependencies": {
    "@types/node": "20.16.5",
    "@typescript-eslint/eslint-plugin": "7.18.0",
    "@typescript-eslint/parser": "7.18.0",
    "eslint": "8.57.1",
    "eslint-config-prettier": "9.1.0",
    "prettier": "3.3.3",
    "tslib": "2.8.1",
    "tsx": "4.17.0",
    "typescript": "5.6.3",
    "vitest": "2.1.4"
  }
}
