{
  "canonicalMetadata": {
    "canonical_version": "AI OSI Stack v5.0.0",
    "canonical_date": "2025-11-09",
    "aeip_version": "1.3",
    "repository_of_record": "https://github.com/ai-osi/ai-osi-stack",
    "domain_of_record": "https://aiosi.org",
    "supersedes_all_prior_metadata": true
  },
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Curated Data Stewardship Manifest",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "uuid",
    "layer",
    "timestamp",
    "custodian",
    "context_uri",
    "evidence_hash",
    "signature",
    "tags",
    "artifact_type",
    "schema_version",
    "payload_uri",
    "schema_ref",
    "datasets"
  ],
  "properties": {
    "uuid": {
      "type": "string"
    },
    "layer": {
      "type": "string",
      "enum": [
        "L2"
      ]
    },
    "timestamp": {
      "type": "string"
    },
    "custodian": {
      "type": "string"
    },
    "context_uri": {
      "type": "string"
    },
    "evidence_hash": {
      "type": "string"
    },
    "signature": {
      "type": "string"
    },
    "schema_ref": {
      "type": "string",
      "enum": [
        "ccm.schema.json"
      ]
    },
    "schema_version": {
      "type": "string"
    },
    "artifact_type": {
      "type": "string",
      "enum": [
        "data-stewardship"
      ]
    },
    "payload_uri": {
      "type": "string"
    },
    "tags": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "provenance_system": {
      "type": "string"
    },
    "datasets": {
      "type": "array",
      "items": {
        "type": "object",
        "required": [
          "name",
          "uri",
          "governance_tier"
        ],
        "properties": {
          "name": {
            "type": "string"
          },
          "uri": {
            "type": "string"
          },
          "governance_tier": {
            "type": "string"
          },
          "update_frequency": {
            "type": "string"
          },
          "quality_checks": {
            "type": "array",
            "items": {
              "type": "string"
            }
          }
        }
      }
    }
  }
}
