{
  "canonicalMetadata": {
    "canonical_version": "AI OSI Stack v5",
    "canonical_date": "2025-11-07",
    "aeip_version": "1.3",
    "repository_of_record": "https://github.com/danielpmadden/ai-osi-stack",
    "domain_of_record": "https://aiosi.org",
    "supersedes_all_prior_metadata": true
  },
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Trusted Execution and Compute Log",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "uuid",
    "layer",
    "timestamp",
    "custodian",
    "context_uri",
    "evidence_hash",
    "signature",
    "tags",
    "artifact_type",
    "schema_version",
    "payload_uri",
    "schema_ref",
    "compute_environment",
    "attestation"
  ],
  "properties": {
    "uuid": {
      "type": "string"
    },
    "layer": {
      "type": "string",
      "enum": [
        "L1"
      ]
    },
    "timestamp": {
      "type": "string"
    },
    "custodian": {
      "type": "string"
    },
    "context_uri": {
      "type": "string"
    },
    "evidence_hash": {
      "type": "string"
    },
    "signature": {
      "type": "string"
    },
    "schema_ref": {
      "type": "string",
      "enum": [
        "tecl.schema.json"
      ]
    },
    "schema_version": {
      "type": "string"
    },
    "artifact_type": {
      "type": "string",
      "enum": [
        "trusted-execution"
      ]
    },
    "payload_uri": {
      "type": "string"
    },
    "tags": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "compute_environment": {
      "type": "object",
      "required": [
        "provider",
        "region",
        "instance_type"
      ],
      "properties": {
        "provider": {
          "type": "string"
        },
        "region": {
          "type": "string"
        },
        "instance_type": {
          "type": "string"
        },
        "boot_hash": {
          "type": "string"
        },
        "kernel_version": {
          "type": "string"
        }
      }
    },
    "attestation": {
      "type": "object",
      "required": [
        "method",
        "evidence",
        "expires_at"
      ],
      "properties": {
        "method": {
          "type": "string"
        },
        "evidence": {
          "type": "string"
        },
        "expires_at": {
          "type": "string"
        },
        "trust_vector": {
          "type": "array",
          "items": {
            "type": "string"
          }
        }
      }
    }
  }
}
